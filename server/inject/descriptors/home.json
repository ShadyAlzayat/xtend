{
  "type": "object",
  "comment": "home screen model",
  "properties": {
    "batchnumber": {
      "type": "string",
      "selector": "#txtinfo",
      "control": "text"
    },
    "submit": {
      "type": "action",
      "selector": ".btn.btn-primary.preventMultiClick.btn-first",
      "control": "action.button"
    },
    "ishome": {
      "type": "identifier",
      "selector": "h3",
      "control": "exists",
      "selectContains": "Enter the Batch Number"
    }
  },
  "connect": [
    {
      "match": [
        "ishome"
      ],
      "action": "submit"
    }
  ]
}
